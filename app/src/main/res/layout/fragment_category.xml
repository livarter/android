<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".main.MyPageFragment"
    android:background="@color/background_color">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_marginTop="30dp">
    <LinearLayout
        android:id="@+id/bed_btn"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        >

        <ImageView
            android:id="@+id/bed_img"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginStart="27dp"
            android:fontFamily="sans-serif-black"
            android:src="@drawable/ic_bed" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/bed_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="27dp"
            android:fontFamily="sans-serif-black"
            android:text="침대"
            android:textSize="19sp" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="7" />

        <ImageView
            android:id="@+id/bed_move"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginEnd="10dp"
            android:src="@drawable/ic_next" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/medium_grey"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
       />
    <LinearLayout
        android:id="@+id/chair_btn"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        >

        <ImageView
            android:id="@+id/chair_img"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginStart="27dp"
            android:fontFamily="sans-serif-black"
            android:src="@drawable/ic_chair" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/chair_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="27dp"
            android:fontFamily="sans-serif-black"
            android:text="의자"
            android:textSize="19sp" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="7" />

        <ImageView
            android:id="@+id/chair_move"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginEnd="10dp"
            android:src="@drawable/ic_next" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/medium_grey"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
    <LinearLayout
        android:id="@+id/desk_btn"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        >

        <ImageView
            android:id="@+id/desk_img"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="27dp"
            android:fontFamily="sans-serif-black"
            android:src="@drawable/ic_desk" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/desk_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="27dp"
            android:fontFamily="sans-serif-black"
            android:text="데스크"
            android:textSize="19sp" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="7" />

        <ImageView
            android:id="@+id/desk_move"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginEnd="10dp"
            android:src="@drawable/ic_next" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/medium_grey"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
    <LinearLayout
        android:id="@+id/cabinet_btn"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        >

        <ImageView
            android:id="@+id/cabinet_img"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginStart="27dp"
            android:fontFamily="sans-serif-black"
            android:src="@drawable/ic_cabinet" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/cabinet_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="27dp"
            android:fontFamily="sans-serif-black"
            android:text="수납장"
            android:textSize="19sp" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="7" />

        <ImageView
            android:id="@+id/cabinet_move"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginEnd="10dp"
            android:src="@drawable/ic_next" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/medium_grey"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
    <LinearLayout
        android:id="@+id/deco_btn"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        >

        <ImageView
            android:id="@+id/deco_img"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginStart="27dp"
            android:fontFamily="sans-serif-black"
            android:src="@drawable/ic_deco" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/deco_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="27dp"
            android:fontFamily="sans-serif-black"
            android:text="홈데코"
            android:textSize="19sp" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="7" />

        <ImageView
            android:id="@+id/deco_move"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginEnd="10dp"
            android:src="@drawable/ic_next" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/medium_grey"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
</LinearLayout>




</androidx.constraintlayout.widget.ConstraintLayout>